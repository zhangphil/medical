<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white" >
    
    <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="fill_parent"
        android:background="@drawable/navigation_bar"
        android:layout_height="70dp" >

        <ImageView
            android:id="@+id/complaints_img_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:layout_centerVertical="true"
            android:padding="10dp"
            android:contentDescription="@null"
            android:src="@drawable/selector_btn_back" />

        <TextView
            style="@style/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/complaints_center" />
        
        <ImageView
            android:id="@+id/complaints_img_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:layout_centerVertical="true"
            android:paddingRight="20dp"
            android:paddingLeft="10dp"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:layout_alignParentRight="true"
            android:contentDescription="@null"
            android:src="@drawable/selector_btn_edit" />
        
    </RelativeLayout>
    
   
    
    <com.wemo.medical.view.CommentListView 
        android:id="@+id/complaints_listview"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_title">
        
    </com.wemo.medical.view.CommentListView>

</RelativeLayout>
